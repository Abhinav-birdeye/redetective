{
	"name": "redis-scanner",
	"version": "1.0.0",
	"description": "A simple command line script to analyse redis keys OR migrate keys from one redis instance to another",
	"author": {
		"name": "Abhinav Thapa",
		"email": "abhinav.thapa@birdeye.com"
	},
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "tsx --env-file=.env src/index.ts",
		"migrate": "tsx --env-file=.env src/migrate.ts",
		"migrate:dev": "tsx watch --env-file=.env src/migrate.ts",
		"scan": "tsx --env-file=.env src/scan.ts",
		"scan:dev": "tsx watch --env-file=.env src/scan.ts",
		"format": "biome format --write ./src/**",
		"lint": "biome lint --write --unsafe ./src/**"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.10",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.4.0",
		"commander": "^13.1.0",
		"ioredis": "^5.6.0",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0",
		"tslib": "^2.8.1",
		"tsx": "^4.19.3"
	}
}